@article{2020EssickiDQStatisticalInference,
  ids = {essick2020idq},
  title = {{{iDQ}}: {{Statistical}} Inference of Non-Gaussian Noise with Auxiliary Degrees of Freedom in Gravitational-Wave Detectors},
