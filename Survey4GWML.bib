  file = {/Users/herb/Zotero/storage/KD38YUP5/2022Colgan_et_al-Architectural_Optimization_and_Feature_Learning_for_High-Dimensional_Time.pdf;/Users/herb/Zotero/storage/PPIANKVI/2202.html}
