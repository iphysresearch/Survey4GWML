@article{2018FunaiThermodynamicsFeatureExtraction,
  ids = {2020},
  title = {Thermodynamics and Feature Extraction by Machine Learning},
  author = {Funai, Shotaro Shiba and Giataganas, Dimitrios},
  year = {2020},
  month = sep,
  journal = {Physical Review Research},
  volume = {2},
